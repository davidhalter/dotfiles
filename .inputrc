# want vi to be the default editor for readline
set editing-mode vi

# vi settings
$if mode=vi
    set keymap vi-command
    "gg": beginning-of-history
    "G": end-of-history
    set keymap vi-insert
    "jj": vi-movement-mode
    #"\C-w": backward-kill-word
    #"\C-p": history-search-backward
$endif
